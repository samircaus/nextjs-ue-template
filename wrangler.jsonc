{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",
  "name": "nextjs-ue-template",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  // Set values in Cloudflare dashboard (Workers → Settings → Variables) or replace here. Do not commit secrets.
  "vars": {
    "AEM_PUBLISH_URL": "",
    "AEM_AUTHOR_URL": "",
    "AEM_PREVIEW_MODE": "",
    "AEM_DEBUG": ""
  }
}
